.wrapper {
  display: flex;
  position: fixed;
  width: 100%;
  height: 110rpx;
  background: #fff;
  border-top: #2f2f2f2f solid .05em;
  /*overflow: visible;*/
  bottom: 0;
  left: 0;
  z-index: 999;
  font-size: 22rpx;
  flex-direction: row;
  justify-content: space-around;
}
.cell {
  position: relative;
  text-align: center;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  display: flex;
  flex: 1;
  height: 100%
}

.path-icon {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 5rpx
}

.path-text {
  color: #515151;
}
.selected-text {
  color: #2ECC71;
}

.center-button {
  position: relative;
  top: -10rpx; /* check the height of the parent when the top attribute doesn't work. */
  z-index: 9999;
  width: 80rpx;
  height: 80rpx
}

.popup {
  position: fixed;
  width: 100%;
  height: 250rpx;
  background: #fff;
  overflow: visible;
  bottom: -250rpx;
  z-index: 1000;
  /* transform: translateY(-250rpx); */
  z-index: 1000;
}

.middle-wrapper {
  width: 100%;
  height: 80%;          /* if not set, the two icon won't show up. */
  position: relative;
}

.popup-cell {
  width: 50%;
  position: absolute;
  text-align: center;
}

.single-cell {
  width: 50%;
  position: relative;
  text-align: center;
  margin: 0 auto;
}

.cell-text {
  font-size: .8em;
  color: #515151;
}

.popup .popup-cell:nth-child(1) {
  left: 0%;
}

.popup .popup-cell:nth-child(2) {
  left: 50%;
}

.popup-icon {
  position: relative;
  width: 20%;
  left: 50%;
  transform: translateX(-50%);
  margin: 30rpx 0 30rpx 0;
}

.bottom-cell {
  width: 4%;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 30rpx;
}
